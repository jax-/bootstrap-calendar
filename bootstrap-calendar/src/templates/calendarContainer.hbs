<div class="calendar-alertarea"></div>
<div class="calendar-panel calendar-panel-top"></div>
{{#if CONFIG.tabbedEventList}}

<ul class="nav nav-tabs nav-calendar">
    <li class="active"><a id="calendar-tab-btn" href="#calendar-tab" data-toggle="tab">{{{RESOURCES.CALENDAR}}}</a></li>
    <li><a id="event-tab-btn" href="#events-tab" data-toggle="tab">{{{RESOURCES.EVENT_LIST}}}</a></li>
</ul>

<div class="tab-content">
    <div class="tab-pane active" id="calendar-tab">
        <div class="calendar-container"></div>
    </div>
    <div class="tab-pane" id="events-tab">
        <div class="event-container"></div>
    </div>
</div>

{{else}}
<div class="calendar-container"></div>
<hr />
<div class="event-container"></div>
{{/if}}

<div class="calendar-panel calendar-panel-bottom"></div>

<div class="calendar-loader"></div> 